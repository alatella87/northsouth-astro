---
// @ts-ignore
import Container from "@components/container.astro";
import Casestudies from "@components/casestudies.astro";
import Servizi from "@components/servizi.astro";
import Hero from "@components/hero.astro";
import Logos from "@components/logos.astro";
import Layout from "@layouts/Layout.astro";
import Marquee from "@components/marquee.astro";
import Form from "@components/form.astro";
import Team from "@components/team.astro";
import { getCollection } from "astro:content";
import ale from "@assets/ale.png";
import zunini from "@assets/zunini.png";
import john from "@assets/john.png";
const publishedTeamMembers = await getCollection("team", ({ data }) => {
  return !data.draft && data.publishDate < new Date();
});
---

<html>
  <head>
    <meta name="title" content="Northsouth - Solutions digitales" />
    <meta
      name="description"
      content="Northsouth offre soluzioni digitali complete e innovative per il successo del tuo business, dall'analisi strategica allo sviluppo tecnologico su misura."
    />
    <meta
      name="keywords"
      content="Soluzioni digitali,Innovazione tecnologica,Sviluppo software,Analisi strategica,Trasformazione digitale,Marketing digitale,Consulenza IT,E-commerce,Sviluppo web,Gestione progetti,UX/UI design,Digital strategy,Business digitale,Servizi IT,Tecnologia su misura"
    />
    <!-- OG Image -->
    <meta property="og:image" content="https://i.ibb.co/w71YW8G/OG-Image.png" />
    <meta name="robots" content="index, follow" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="language" content="English" />
    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-JYJXYQEZ3Y"
    ></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag("js", new Date());
      gtag("config", "G-JYJXYQEZ3Y");
    </script>
  </head>
</html>

<!-- <Layout title="Northsouth - Soluzioni digitali a 360 gradi"> -->
  <Container>
    <section class="relative isolate overflow-hidden bg-white px-6 pb-6 lg:px-8">
      <div
        class="absolute inset-0 -z-10 bg-[radial-gradient(45rem_50rem_at_top,theme(colors.indigo.100),white)] opacity-20">
      </div>
      <div
        class="absolute inset-y-0 right-1/2 -z-10 mr-16 w-[200%] origin-bottom-left skew-x-[-30deg] bg-white shadow-xl shadow-indigo-600/10 ring-1 ring-indigo-50 sm:mr-28 lg:mr-0 xl:mr-16 xl:origin-center">
      </div>
      <div class="mx-auto max-w-2xl lg:max-w-4xl">
        <img class="mx-auto h-12 opacity-50" />
        <div class="text-center lg:my-48 md:my-12 sm:my-12">
          <h1
            class="text-3xl lg:text-4xl xl:text-5xl font-bold lg:tracking-tight xl:tracking-tighter">
            <span class="text-indigo-600">Northsouth</span>
          </h1>
          <h1
            class="mt-4 text-3xl lg:text-4xl xl:text-5xl font-bold lg:tracking-tight xl:tracking-tighter">
            Solutions digitales à 360 degrés.
          </h1>
          <p
            class="mt-8 font-light text-center text-gray-500 dark:text-gray-400 sm:text-xl">
            Northsouth faît croître votre entreprise en vous faisant économiser des ressources..<br />
          </p>
          <div class="-space-x-2 mt-8">
            <img
              class="inline-block size-[46px] rounded-full ring-2 ring-white dark:ring-neutral-900"
              src={zunini.src}
              alt="Avatar"
            />
            <img
              class="inline-block size-[46px] rounded-full ring-2 ring-white dark:ring-neutral-900"
              src={john.src}
              alt="Avatar"
            />
            <img
              class="inline-block size-[46px] rounded-full ring-2 ring-white dark:ring-neutral-900"
              src={ale.src}
              alt="Avatar"
            />
          </div>
          <a
            target="_blank"
            class="inline-flex justify-between items-center py-1 px-1 pr-4 mb-2 mt-7 text-sm text-gray-700 bg-gray-100 rounded-full dark:bg-gray-800 dark:text-white hover:bg-gray-200 dark:hover:bg-gray-700"
            role="alert">
            <span
              class="bg-indigo-700 text-sm bg-primary-600 rounded-full text-white px-4 py-1.5 mr-3"
              >en <b>Valais</b></span
            >
            <span class="text-sm font-medium">App, E-Commerce, CMS, AI pour tous.</span>
          </a>
          <div class="mt-12">
            <a href="https://tally.so/r/3yv7l8" class="text-white bg-red-700 hover:bg-blue-800 focus:ring-4 focus:ring-blue-300 font-medium rounded-lg text-sm px-5 py-2.5 dark:bg-blue-600 dark:hover:bg-blue-700 focus:outline-none dark:focus:ring-blue-800">Concours Foire du Valais 2024 ></a>
          </div>
        </div>

      </div>
    </section>


    
    <div class="overflow-hidden bg-white py-24 sm:py-32">
      <div class="mx-auto max-w-7xl px-6 lg:px-8">
        <div class="mx-auto grid max-w-2xl grid-cols-1 gap-x-8 gap-y-16 sm:gap-y-20 lg:mx-0 lg:max-w-none lg:grid-cols-2">
          <div class="lg:pr-8 lg:pt-4">
            <div class="lg:max-w-lg">
              <h2 class="text-base font-semibold leading-7 text-indigo-600">Plus rapidement en ligne</h2>
              <p class="mt-2 text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl">Un meilleur workflow</p>
              <p class="mt-6 text-lg leading-8 text-gray-600">Avec nos technologies partenaires, nous vous soutenons sans délais par rapports à vos environnement test.</p>
              <dl class="mt-10 max-w-xl space-y-8 text-base leading-7 text-gray-600 lg:max-w-none">
                <div class="relative pl-9">
                  <dt class="inline font-semibold text-gray-900">
                    <svg class="absolute left-1 top-1 h-5 w-5 text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                      <path fill-rule="evenodd" d="M5.5 17a4.5 4.5 0 01-1.44-8.765 4.5 4.5 0 018.302-3.046 3.5 3.5 0 014.504 4.272A4 4 0 0115 17H5.5zm3.75-2.75a.75.75 0 001.5 0V9.66l1.95 2.1a.75.75 0 101.1-1.02l-3.25-3.5a.75.75 0 00-1.1 0l-3.25 3.5a.75.75 0 101.1 1.02l1.95-2.1v4.59z" clip-rule="evenodd" />
                    </svg>
                    De l'idée à la réalisation.
                  </dt>
                  <dd class="inline">Choisissez une approche custom, ou out-of-the box avec nos solutions partenaires E-commerce, CMS, ERP, Knowledge Management, AI, Automatisation.</dd>
                </div>
                <div class="relative pl-9">
                  <dt class="inline font-semibold text-gray-900">
                    <svg class="absolute left-1 top-1 h-5 w-5 text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                      <path fill-rule="evenodd" d="M10 1a4.5 4.5 0 00-4.5 4.5V9H5a2 2 0 00-2 2v6a2 2 0 002 2h10a2 2 0 002-2v-6a2 2 0 00-2-2h-.5V5.5A4.5 4.5 0 0010 1zm3 8V5.5a3 3 0 10-6 0V9h6z" clip-rule="evenodd" />
                    </svg>
                    FADP + GDPR sur vos propriétés. 
                  </dt>
                  <dd class="inline">Nous vous soutenons à travers le choix parmis différents providers.</dd>
                </div>
                <div class="relative pl-9">
                  <dt class="inline font-semibold text-gray-900">
                    <svg class="absolute left-1 top-1 h-5 w-5 text-indigo-600" viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                      <path d="M4.632 3.533A2 2 0 016.577 2h6.846a2 2 0 011.945 1.533l1.976 8.234A3.489 3.489 0 0016 11.5H4c-.476 0-.93.095-1.344.267l1.976-8.234z" />
                      <path fill-rule="evenodd" d="M4 13a2 2 0 100 4h12a2 2 0 100-4H4zm11.24 2a.75.75 0 01.75-.75H16a.75.75 0 01.75.75v.01a.75.75 0 01-.75.75h-.01a.75.75 0 01-.75-.75V15zm-2.25-.75a.75.75 0 00-.75.75v.01c0 .414.336.75.75.75H13a.75.75 0 00.75-.75V15a.75.75 0 00-.75-.75h-.01z" clip-rule="evenodd" />
                    </svg>
                    Adaptés à votre situation
                  </dt>
                  <dd class="inline">Besoin d'externaliser un processus? Nous commençons par ce dont vous avez le plus besoin.</dd>
                </div>
              </dl>
            </div>
          </div>
          <img src="https://tailwindui.com/img/component-images/dark-project-app-screenshot.png" alt="Product screenshot" class="w-[48rem] max-w-none rounded-xl shadow-xl ring-1 ring-gray-400/10 sm:w-[57rem] md:-ml-4 lg:-ml-0" width="2432" height="1442">
        </div>
      </div>
    </div>

    <div class="text-left mt-16 mb-4">
      <section class="bg-white dark:bg-gray-900">
        <div class="py-8 lg:py-16 px-4 mx-auto max-w-screen-md">
          <h2
            class="mb-4 text-4xl tracking-tight font-extrabold text-center text-gray-900 dark:text-white">
            Nous contacter
          </h2>
            <p
            class="font-light text-center text-gray-500 dark:text-gray-400 sm:text-xl">
            Nous serons ravis de recevoir et d'analyser votre demande d'informations, de proposition digitale ou de question technique.
          </p>
        </div>
        <div class="mt-4">
          <iframe
            title="Contact Form"
            src="https://plugins.crisp.chat/urn:crisp.im:contact-form:0/contact/727a3632-d1bc-44a3-9179-65f86310a145"
            referrerpolicy="origin"
            sandbox="allow-forms allow-popups allow-scripts allow-same-origin"
            width="100%"
            height="700px"
            frameborder="0">
          </iframe>
        </div>
      </section>
    </div>
    <!-- <Servizi />
    <Casestudies />
    <Marquee />
    <Form />
    <Logos />
    <Team /> -->
  </Container>
<!-- </Layout> -->

<style>
  .gradient {
    background: linear-gradient(to right, rgb(255, 0, 0), transparent 90%);
  }
  .animate-marquee {
    animation-delay: 1s;
  }
</style>

<script type="text/javascript">
  var _iub = _iub || [];
  _iub.csConfiguration = {
